        <!-- Main Content -->
        <main class="main-content">
            <!-- Metrics Grid -->
            <section class="metrics-grid">
                <div class="metric-card glass slide-in" style="animation-delay: 0.1s">
                    <div class="metric-header">
                        <div class="metric-title">Quantum Processing</div>
